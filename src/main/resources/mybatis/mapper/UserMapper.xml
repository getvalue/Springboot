<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gmbsh.Mapper.UserMapper" >

    <select id="findAll" resultType="User">
        SELECT user_id,username,password,role_id  FROM SYS_USER
    </select>

    <select id="findOne" parameterType="String" resultType="User">
        SELECT user_id,username,password,role_id  FROM SYS_USER where user_id=#{id}
    </select>

</mapper>